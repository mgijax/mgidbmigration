#!/bin/csh -f

#setenv TRANSLOAD        /usr/local/mgi/3.0/dataload/translationload
setenv TRANSLOAD        /home/lec/loads/translationload
#setenv SETLOAD		/usr/local/mgi/3.0/dataload/setload
setenv SETLOAD		/home/lec/loads/setload
#setenv CACHELOAD	/usr/local/mgi/3.0/dbutils/seqcacheload
setenv CACHELOAD	/home/lec/loads/seqcacheload
#setenv MRKREFLOAD	/usr/local/mgi/3.0/dbutils/mrkrefload
setenv MRKREFLOAD	/home/lec/loads/mrkrefload-jsam

# use current MGI 2.98 version of vocload because we load
# the vocabularies BEFORE converting to the new schema
setenv VOCLOAD          /home/lec/loads/vocload-2.98/runSimpleFullLoadNoArchive.sh

# existing (MGI 2.98) schema configured to talk to an MGI 2.98 database
setenv existingmgddbschema /home/lec/db/mgi2.98/mgddbschema

# existing (MGI 2.98) schema configured to talk to the JSAM database
setenv oldmgddb /home/lec/db/jsam2.98
setenv oldmgddbschema ${oldmgddb}/mgddbschema
setenv oldmgddbperms ${oldmgddb}/mgddbperms

# new (JSAM) schema configured to talk to the JSAM database
setenv newmgddb /home/lec/db
setenv newmgddbschema ${newmgddb}/mgddbschema
setenv newmgddbperms ${newmgddb}/mgddbperms

# new (JSAM) RADAR schema configured to talk to JSAM RADAR database
setenv newrdrdb /home/lec/db
setenv newrdrdbschema ${newrdrdb}/radardbschema
setenv newrdrdbperms ${newrdrdb}/radardbperms

# the working directory
setenv WORKDIR  /home/lec/db/mgidbmigration/JSAM/work

# the path where the bcp files have been installed
setenv DATADIR  ${WORKDIR}/data

setenv CREATEDBY	1200
setenv UNKNOWNEDITOR	1111
setenv SCHEMA_TAG	mgddbschema-8-0-0
setenv PUBLIC_VERSION	"MGI 3.0"
setenv PRODUCT_NAME	MGD
setenv RADAR_SCHEMA_TAG	radardbschema-2-0-0

source ${newmgddbschema}/Configuration

